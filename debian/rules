clean:
	dh_testdir
	dh_clean

binary-arch:
	dh_testdir
	dh_testroot
	dh_prep
	dh_installdirs
	dh_install
	dh_link
	dh_installinit --no-start --name=hipache
	dh_strip
	dh_compress
	dh_fixperms
	dh_installdeb
	dh_gencontrol
	dh_md5sums
	dh_builddeb

binary: binary-arch
